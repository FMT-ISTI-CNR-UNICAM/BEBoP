			</bpmn2:extensionElements>
		</bpmn2:serviceTask> -->

		<bpmn2:intermediateThrowEvent id="SendTask_1">
			<bpmn2:messageEventDefinition messageRef="Message_1" />
		</bpmn2:intermediateThrowEvent>

		<bpmn2:intermediateCatchEvent id="waitTask">
