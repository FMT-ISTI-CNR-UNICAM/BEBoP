		<bpmn2:messageFlow
			id="MessageFlow_2"
			messageRef="Message_2"
			sourceRef="SendTask_2"
			targetRef="waitTask" />
	</bpmn2:collaboration>

	<bpmn2:process
		id="Process_1"
		name="Initiating Process"
		definitionalCollaborationRef="Collaboration_1">

		<bpmn2:startEvent
			id="StartEvent_1"
			name="Start Event 1">
		</bpmn2:startEvent>

		<bpmn2:userTask
			id="UserTask_1"
			name="User Task 1">
			<bpmn2:documentation>Some doc</bpmn2:documentation>
			<bpmn2:potentialOwner>
				<bpmn2:resourceAssignmentExpression>
					<bpmn2:formalExpression>User1</bpmn2:formalExpression>
				</bpmn2:resourceAssignmentExpression>
			</bpmn2:potentialOwner>
		</bpmn2:userTask>

